DOTENV_SILENT=true      #Stop dotenv from outputting ads
NODE_ENV=production     #Environment mode
LOG_LEVEL=info          #Log level: error, warn, info, verbose, debug, silly
WORKER_LOG_LEVEL=info   #Worker lifecycle log level (defaults to info)
SC_WORKER_SILENT_CONSOLE= #Set to 1/0 to force worker console logging on/off
KIT_RPC_MAX_RETRIES=1   #Retry count for retryable RPC reads (0 disables retries)
KIT_RPC_RETRY_BASE_MS=200   #Base backoff delay for RPC retries (ms)
KIT_RPC_RETRY_MAX_MS=2000   #Max backoff delay for RPC retries (ms)
KIT_RPC_LOG_PAYLOAD=    #Set to "full" to log full RPC payloads

OPENAI_API_KEY=         #You need an OpenAI API key to use the AI features
OPENAI_RESPONSES_MODEL=gpt-4.1-mini     #Defaults to gpt-4.1-mini if unset
DOSSIER_VECTOR_STORE_ID=   #Vector store for dossier analysis (optional)
AUTOPSY_VECTOR_STORE_ID=   #Vector store for autopsy analysis (optional)
xAI_API_KEY=           #xAI API key for Grok-powered AI jobs (DevScan summaries)
DEVSCAN_RESPONSES_MODEL= #Optional: override DevScan AI model (defaults to grok-4-1-fast-reasoning)

# Add an old wallet here for testing/trading
TEST_TRADER=    #Trader name
TEST_PUBKEY=    #Test Wallet Address

# === DATABASE ===
# Requires a running SQLite instance
DB_ENGINE=sqlite
BOOTYBOX_SQLITE_PATH=db/test-data.db
BOOTYBOX_LOG_LEVEL=debug

# SolanaTracker.io is the EXCLUSIVE supported RPC provider for all VAULT77 artifacts
# Get an account here: https://www.solanatracker.io/?ref=0NGJ5PPN
SOLANATRACKER_API_KEY=      #You'll get an API Key when you sign up for a Data API Plan
SOLANATRACKER_URL=https://data.solanatracker.io
SOLANATRACKER_RPC_HTTP_URL=     #You'll also need an RPC account to get direct blockchain data
SOLANATRACKER_RPC_WS_URL=
SWAP_API_PROVIDER=swapV3        #Swap engine provider: swapV3 (default) or raptor
DEVSCAN_API_KEY=                #DevScan public API key for token/developer lookups

# Optional proxy settings for HTTP/WS access
HTTPS_PROXY=
HTTP_PROXY=
NO_PROXY=

# Warchest HUD display limits
WARCHEST_HUD_MAX_TX=10         # Max recent transactions to display
WARCHEST_HUD_MAX_LOGS=5        # Max recent logs per wallet to display
WARCHEST_TARGET_LIST_INTERVAL_MS=300000   # Target list timer (ms); use OFF to disable
WARCHEST_WS_STALE_MS=20000     # Restart when no slot update for this long
WARCHEST_WS_RESTART_GAP_MS=30000
WARCHEST_WS_RESTART_MAX_BACKOFF_MS=300000
WARCHEST_WS_UNSUB_TIMEOUT_MS=2500

SAVE_PARSED=FALSE       #Save parsed files to /data
SAVE_ENRICHED=FALSE     #Save enriched files to /data
SAVE_RAW=FALSE          #Save raw files to /data
HARVEST_LIMIT=100    #Max number of records to fetch per harvest job
FEATURE_MINT_COUNT=8    #Default number of recent mints to summarize for dossier features
